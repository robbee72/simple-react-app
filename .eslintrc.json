{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "mocha": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["react"],
  "extends": ["react-app", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }],

    "no-console": 0
  }
}
